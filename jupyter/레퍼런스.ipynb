    "        if not images:\n",
    "            result_info[\"status\"] = \"FAIL_NO_IMG\"\n",
    "            result_info[\"log_message\"] = f\"FAILED (No Image): {simple_filename}\"\n",
    "            return result_info\n",
    "\n",
    "        images[0].save(save_path)\n",
    "\n",
    "        result_info[\"status\"] = \"SUCCESS\"\n",
    "        result_info[\"log_message\"] = f\"SUCCESS: {simple_filename} (Ref C: {os.path.basename(font_image_path)})\"\n",
    "        return result_info\n",
    "\n",
    "    except Exception as e:\n",
    "        result_info[\"status\"] = \"ERROR_EXCEPTION\"\n",
