    "def get_thread_client() -> genai.Client:\n",
    "    \"\"\"í˜„ì¬ ìŠ¤ë ˆë“œ ì „ìš© í´ë¼ì´ì–¸íŠ¸ë¥¼ ë°˜í™˜\"\"\"\n",
    "    if not hasattr(_thread_local, \"client\"):\n",
    "        _thread_local.client = genai.Client(api_key=API_KEY)\n",
    "    return _thread_local.client\n",
    "\n",
    "def safe_load_image(path: str):\n",
    "    try:\n",
    "        with Image.open(path) as im:\n",
    "            im.load()\n",
    "            if im.mode != \"RGB\":\n",
    "                im = im.convert(\"RGB\")\n",
    "            max_size = 1024\n",
    "            if max(im.size) > max_size:\n",
    "                im.thumbnail((max_size, max_size), Image.Resampling.LANCZOS)\n",
    "            return im.copy()\n",
    "    except Exception:\n",
    "        return None\n",
    "\n",
    "def get_all_files_recursive(root_path: str):\n",
    "    \"\"\"í•˜ìœ„ í´ë”ê¹Œì§€ ëª¨ë“  ì´ë¯¸ì§€ íŒŒì¼ ê²½ë¡œë¥¼ ë°˜í™˜\"\"\"\n",
    "    valid_extensions = (\".png\", \".jpg\", \".jpeg\", \".webp\")\n",
    "    if not os.path.isdir(root_path):\n",
    "        return []\n",
    "    paths = []\n",
    "    for root, _, files in os.walk(root_path):\n",
    "        for filename in files:\n",
    "            if filename.lower().endswith(valid_extensions):\n",
    "                paths.append(os.path.join(root, filename))\n",
    "    return sorted(paths)\n",
    "\n",
    "def get_next_file_number(output_dir: str, prefix: str = \"ref_\") -> int:\n",
    "    \"\"\"ì¶œë ¥ í´ë”ë¥¼ ìŠ¤ìº”í•˜ì—¬ ë‹¤ìŒ ë²ˆí˜¸ë¥¼ ê²°ì •\"\"\"\n",
    "    if not os.path.exists(output_dir):\n",
    "        return 1\n",
    "\n",
    "    max_num = 0\n",
    "    for fname in os.listdir(output_dir):\n",
    "        if fname.startswith(prefix) and fname.endswith(\".png\"):\n",
    "            # ref_0001.png -> 0001 ì¶”ì¶œ\n",
    "            try:\n",
    "                base = os.path.splitext(fname)[0]\n",
    "                num_part = base.replace(prefix, \"\")\n",
    "                num = int(num_part)\n",
    "                if num > max_num:\n",
    "                    max_num = num\n",
    "            except ValueError:\n",
    "                continue\n",
    "    return max_num + 1\n",
    "\n",
    "def extract_pil_images_from_response(response):\n",
    "    images = []\n",
    "    if hasattr(response, \"candidates\"):\n",
    "        for cand in response.candidates:\n",
    "            if hasattr(cand.content, \"parts\"):\n",
    "                for part in cand.content.parts:\n",
    "                    if hasattr(part, \"inline_data\") and part.inline_data:\n",
    "                        from io import BytesIO\n",
    "                        try:\n",
    "                            im = Image.open(BytesIO(part.inline_data.data))\n",
    "                            im.load()\n",
    "                            if im.mode != \"RGB\":\n",
    "                                im = im.convert(\"RGB\")\n",
    "                            images.append(im)\n",
    "                        except Exception:\n",
    "                            pass\n",
    "    return images\n",
    "\n",
    "def call_generate_with_retry(prompt, img_bottle, img_logo, img_font, temperature, top_p):\n",
    "    last_err = None\n",
    "    client = get_thread_client()\n",
    "\n",
    "    for attempt in range(1, MAX_API_ATTEMPTS + 1):\n",
    "        try:\n",
    "            return client.models.generate_content(\n",
    "                model=MODEL_ID,\n",
    "                contents=[prompt, img_bottle, img_logo, img_font],\n",
    "                config=types.GenerateContentConfig(\n",
    "                    response_modalities=[\"TEXT\", \"IMAGE\"],\n",
    "                    temperature=temperature,\n",
    "                    top_p=top_p,\n",
    "                    image_config=types.ImageConfig(\n",
    "                        aspect_ratio=ASPECT_RATIO,\n",
    "                        image_size=IMAGE_SIZE,\n",
    "                    ),\n",
    "                ),\n",
    "            )\n",
    "        except Exception as e:\n",
    "            error_msg = str(e)\n",
    "            last_err = e\n",
    "            if \"429\" in error_msg:\n",
    "                wait_time = 20 * attempt\n",
    "                print(f\"âš ï¸ [Quota Exceeded] 429 ë°œìƒ. {wait_time}ì´ˆ ëŒ€ê¸° ì¤‘... (Attempt {attempt}/{MAX_API_ATTEMPTS})\")\n",
    "                time.sleep(wait_time)\n",
    "                continue\n",
    "            elif \"500\" in error_msg or \"503\" in error_msg:\n",
    "                 wait_time = 5 * attempt\n",
    "                 print(f\"âš ï¸ [Server Error] ì„œë²„ ë¶ˆì•ˆì •. {wait_time}ì´ˆ ëŒ€ê¸° í›„ ì¬ì‹œë„...\")\n",
    "                 time.sleep(wait_time)\n",
    "                 continue\n",
    "            else:\n",
    "                if attempt >= MAX_API_ATTEMPTS:\n",
    "                    break\n",
    "                time.sleep(2)\n",
    "    raise last_err\n",
    "\n",
    "# ==============================================================================\n",
    "# [3. ë©”ì¸ ë¡œì§]\n",
